{
  "name": "viewer",
  "version": "0.0.1",
  "description": "Simple graphos viewer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "win32": "electron-packager . Graphos --platform=win32 --arch=ia32",
    "win64": "electron-packager . Graphos --platform=win64 --arch=x64",
    "linux32": "electron-packager . Graphos --platform=linux --arch=ia32",
    "linux64": "electron-packager . Graphos --platform=linux --arch=x64",
    "osx": "electron-packager . Graphos --platform=darwin --arch=x64",
    "build": "electron-packager . --all"
  },
  "author": "Cesar Gimenes",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.6.2",
    "electron-packager": "^8.6.0",
    "electron-prebuilt": "^1.6.2"
  }
}
